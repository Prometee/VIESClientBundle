{
    "name": "prometee/vies-client-bundle",
    "description": "European VAT Information Exchange System SOAP client bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "Prometee@users.noreply.github.com"
        }
    ],
    "require": {
        "prometee/vies-client": "^1.0",
        "symfony/http-kernel": "^4.4|^5.4|^6.0",
        "symfony/dependency-injection": "^4.4|^5.4|^6.0",
        "symfony/config": "^4.4|^5.4|^6.0",
        "symfony/validator": "^4.4|^5.4|^6.0"
    },
    "require-dev": {
        "ext-soap": "*",
        "phpunit/phpunit": "^8|^9",
        "symplify/easy-coding-standard": "^8|^9|^10|^11",
        "phpstan/phpstan": "^1",
        "vimeo/psalm": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Prometee\\VIESClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Prometee\\VIESClientBundle\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
